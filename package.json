{
  "name": "oaf-react-router",
  "version": "2.1.1",
  "main": "dist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oaf-project/oaf-react-router.git"
  },
  "devDependencies": {
    "@types/history": "^4.7.4",
    "@types/jest": "^25.1.0",
    "history": "^4.10.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "prettylint": "^1.0.0",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "tslint-sonarts": "^1.9.0",
    "type-coverage": "^2.4.0",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "oaf-routing": "^4.0.0"
  },
  "peerDependencies": {
    "history": "^4.10.1"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "format-lint": "prettylint '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "release": "yarn build && yarn lint && yarn type-coverage && yarn format-lint && yarn publish && yarn documentation",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
